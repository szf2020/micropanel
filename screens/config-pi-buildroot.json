    {
      "id": "fpga_12_3_inch",
      "title": "12-3-inch",
      "enabled": false,
      "type": "GenericList",
      "list_items": [
        {
          "title": "Detect",
          "action": "stdbuf -oL -eL /usr/bin/fpga-jtag-flasher.sh --info --skipbitstream",
          "async": true,
          "timeout": 25,
          "log_file": "/tmp/fpga-flash.log",
          "progress_title": "Detecting 12-3-inch..",
          "result_pattern": "DEVICE=",
          "result_prefix": "FPGA: "
        },
        {
          "title": "Update",
          "action": "stdbuf -oL -eL /usr/bin/fpga-jtag-flasher-wrapper.sh --operation=flash --file=12-3-inch --fpga-type=xilinx",
          "async": true,
          "timeout": 350,
          "log_file": "/tmp/fpga-flash.log",
          "progress_title": "Updating 12-3-inch.."
        },
        {
          "title": "Backup",
          "action": "stdbuf -oL -eL /usr/bin/fpga-jtag-flasher-wrapper.sh --operation=dump --file=12-3-inch --fpga-type=xilinx --size=2192012",
          "async": true,
          "timeout": 350,
          "log_file": "/tmp/fpga-flash.log",
          "progress_title": "Backing up 12-3-inch.."
        },
        {
          "title": "Revert",
          "action": "stdbuf -oL -eL /usr/bin/fpga-jtag-flasher-wrapper.sh --operation=revert --file=12-3-inch --fpga-type=xilinx",
          "async": true,
          "timeout": 350,
          "log_file": "/tmp/fpga-flash.log",
          "progress_title": "Reverting 12-3-inch.."
        },
        {
          "title": "Back"
        }
      ]
    },
